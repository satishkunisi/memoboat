<% if (memo) { %>
<%   var title_content = memo.get('title')  %>
<% } else { %>
<%   var title_content = "" %>
<% } %>

<input type="text" 
           class="form-control" 
           id="memo_title" 
           name="memo[title]" 
           placeholder="Just start typing..."
           value="<%= title_content %>">
<br />