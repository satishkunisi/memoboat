<div class="list-group">
  <% notebooks.forEach(function (notebook) { %>
    <a href="#" class="list-group-item" data-id="<%= notebook.id %>">
      <span class="listgroup-item-heading">
        <%= notebook.get('title') %>
      </span>
      <button type="button" data-id="<%= notebook.id %>" class="btn btn-default btn-xs pull-right">
        <span class="caret"></span>
      </button>
    </a>
    <ul class="dropdown-menu pull-right" id="nb-menu-<%= notebook.id %>">
      <li>
        <a role="menuitem"
           data-action="delete"
           data-id="<%= notebook.id %>"
           tabindex="-1" href="#">
           Delete
        </a>
      </li>
    </ul>
  <% }) %>
</div>